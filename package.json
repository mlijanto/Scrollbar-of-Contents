{
  "name": "scrollbar-of-contents",
  "author": "Martin Lijanto",
  "version": "0.3.0",
  "license": "MIT",
  "description": "Scrollbar of Contents is a Chrome extension that creates clickable markers for all the headings of a page and places them proportionately next to the scrollbar.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlijanto/scrollbar-of-contents.git"
  },
  "dependencies": {
    "@material/button": "^3.1.0",
    "@material/checkbox": "^3.1.0",
    "@material/form-field": "^3.1.0",
    "@material/ripple": "^3.1.0",
    "@material/slider": "^3.1.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.88",
    "@types/jest": "^26.0.13",
    "autoprefixer": "^9.6.1",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^26.4.2",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^6.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rimraf build && npx webpack"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
